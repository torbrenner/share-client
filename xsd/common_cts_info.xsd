  elementFormDefault="qualified" targetNamespace="http://schema.samply.de/common"
  version="1.0" xmlns="http://schema.samply.de/common">

  <xs:complexType name="cts">
    <xs:all>
      <xs:element minOccurs="0" name="username" type="xs:string"/>
      <xs:element minOccurs="0" name="password" type="xs:string"/>
      <xs:element minOccurs="0" name="url" type="xs:anyURI"/>
      <xs:element minOccurs="0" name="profile" type="xs:anyURI"/>
      <xs:element minOccurs="0" name="mainzellisteUrl" type="xs:anyURI"/>
      <xs:element minOccurs="0" name="mainzellisteApiKey" type="xs:string"/>
      <xs:element minOccurs="0" name="searchIdType" type="xs:string"/>
    </xs:all>
  </xs:complexType>

  <xs:element name="cts" type="cts"/>
</xs:schema>
